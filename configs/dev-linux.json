        },
        "2": {
            "hostname": "127.0.0.1",
            "servlet": {
                "0": {
                    "_": "evoparsons.broker.NetworkPolicy$InstructorServlet",
                    "path": "/api/instructor/*"
                }
            },
            "www": "file:///home/dvitel/cereal/EvoParsons/www/instructor",
            "port": "58240",
            "_": "evoparsons.broker.NetworkPolicy+REST"
        }
