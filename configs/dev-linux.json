        },
        "events": {
            "_": "evoparsons.repo.EventsMongoRepo",
            "name": "events"
        },
        "students": {
            "_": "evoparsons.repo.MongoPojoRepo$Students",
            "name": "students"
        }
    },
    "log": {
        "header": "[GROUPS]",
        "_": "evoparsons.broker.Log+console"
    },    
    "lib": "evoparsons.broker.ParsonsLibrary",
    "broker": {
        "_": "evoparsons.broker.GroupsBroker",
        "name": "GR",
        "child": {
            "1": {
                "ea": {
                    "preset": [
                        [
                            "ArraysPalindrom.java",
                            "FN_1-.bro", "RR_1-8.bro", "ARR_1-1.bro", "ARR_3-1.bro", "OO_5-2.bro", "OS_1-1.bro", "FN_6-2.bro", "C-CPP-JAVA_Altadmir-2015_IMBALANCED-BRACKETS_5.bro"
