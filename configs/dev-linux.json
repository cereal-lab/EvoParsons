                    ],
                    "_": "evoparsons.broker.PresetStarter"
                },
                "broker": {
                    "distributionPolicy": "evoparsons.broker.SelectionPolicy+cycling",
                    "_": "evoparsons.broker.ParsonsBroker",
                    "name": "PRESET"
                },
                "repo": {
                    "students": {
                        "_": "evoparsons.repo.MongoPojoRepo$Students",
                        "name": "students"
                    }
                },
                "genotypesFile": "preset.genotypes.bro",
                "genotypeEvolFile": "preset.genotypeEvol.bro",
