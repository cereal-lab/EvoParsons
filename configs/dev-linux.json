                    "header": "[PRESET]"
                }
            },
            "2": {
                "studentsStatsFile": "ecj.studentsStats.bro",
                "broker": {
                    "distributionPolicy": "evoparsons.broker.SelectionPolicy+cycling",
                    "_": "evoparsons.broker.ParsonsBroker",
                    "name": "ECJ"
                },
                "repo": {
                    "students": {
                        "_": "evoparsons.repo.MongoPojoRepo$Students",
                        "name": "students"
                    }
                },
                "output": "./DATA.out/ecj.out",
                "ea": { 
                    "_": "evoparsons.broker.ECJStarter",
                    "state": "evoparsons.ecj.ParsonsEvolutionState",
                    "pop": {
                        "subpop": {
                            "0": {
                                "_": "ec.Subpopulation",
                                "species": {
                                    "fitness": "ec.simple.SimpleFitness",
                                    "mutation-prob": "1.0",
                                    "pipe": {
                                        "_": "evoparsons.ecj.MutatorEvoParsons",
                                        "source": {
                                            "0": "evoparsons.ecj.EverySelection"
                                        }
                                    },
                                    "ind": "ec.vector.IntegerVectorIndividual",
                                    "genome-size": "10",
                                    "_": "ec.vector.IntegerVectorSpecies"
                                },
                                "duplicate-retries": "1",
                                "size": "2"
                            }
                        },
                        "_": "ec.Population",
                        "subpops": "1"
                    },
                    "stat": {
                        "_": "ec.simple.SimpleStatistics",
                        "num-children": "1",
                        "child": {
                            "0": {
                                "do-subpops": "true",
                                "file": "$./DATA.out/customEvoParsons.stat",
                                "_": "evoparsons.ecj.CustomStatistics"
                            }
                        },
                        "file": "$./DATA.out/evoParsons.stat"
                    },  
                    "init": "ec.simple.SimpleInitializer",
                    "checkpoint": "true",
                    "print-unaccessed-params": "true",
                    "checkpoint-directory": "$./DATA.out",
                    "eval": {
                        "style": "round-robin",
                        "problem": "evoparsons.ecj.EvoParsonsProblem",
                        "_": "ec.simple.SimpleEvaluator"
                    },
                    "checkpoint-modulo": "1",
                    "breedthreads": "1",
                    "evalthreads": "1",
                    "jobs": "50",
                    "generations": "500",
                    "checkpoint-prefix": "ec",
                    "finish": "ec.simple.SimpleFinisher",
                    "breed": "ec.simple.SimpleBreeder",
                    "exch": "ec.simple.SimpleExchanger",
                    "quit-on-run-complete": "true",
                    "seed": {
                        "0": "4359"
                    }        
                },
                "genotypesFile": "ecj.genotypes.bro",
                "evalTries": "2",
                "genotypeEvolFile": "ecj.genotypeEvol.bro",
                "studentsFile": "ecj.students.bro",
                "log": {
                    "header": "[ECJ]"
                }
            }
        }
    }
}